{
  "name": "altairx-asm",
  "displayName": "AltairX ASM",
  "description": "Basic Syntax Highlighting for AltairX assembly language",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": {
    "url": "https://github.com/Alairion/AltairX-ASM-VSC.git"
  },
  "contributes": {
    "languages": [{
      "id": "altairxasm",
      "aliases": ["AltairX ASM", "altairxasm"],
      "extensions": [".asm",".s"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "altairxasm",
      "scopeName": "source.altairx",
      "path": "./syntaxes/altairxasm.tmLanguage.json"
    }]
  }
}
